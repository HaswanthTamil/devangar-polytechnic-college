{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "required": [
            "title",
            "file",
            "year",
            "visible"
        ],
        "additionalProperties": false,
        "properties": {
            "title": {
                "type": "string"
            },
            "file": {
                "type": "string"
            },
            "year": {
                "type": "integer",
                "minimum": 2000
            },
            "visible": {
                "type": "boolean"
            }
        }
    }
}
